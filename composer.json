{
    "name": "jlab/epas",
    "description": "A package to aid in the local management of Plant Items that will be synchronized to ePAS.",
    "license": "MIT",
    "authors": [
        {
            "name": "Theo Larrieu",
            "email": "theo@jlab.org"
        }
    ],
    "homepage": "https://github.com/jlab/epas",
    "keywords": ["Laravel", "Epas"],
    "require": {
        "illuminate/support": "^10.0",
        "jlab/laravel-utilities": "^10.0",
        "laravel/legacy-factories": "^1.1",
        "tightenco/ziggy": "^1.1",
        "maatwebsite/excel": "^3.1.50",
        "ext-pdo": "*",
        "laravel/sail": "^1.19",
        "rap2hpoutre/fast-excel": "^5.3",
        "laravel/scout": "^10.6",
        "babenkoivan/elastic-scout-driver": "^3.1",
        "babenkoivan/elastic-scout-driver-plus": "^4.6",
        "inertiajs/inertia-laravel": "^0.6.11",
        "yajra/laravel-oci8": "^10.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "8.*",
        "sti3bas/laravel-scout-array-driver": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Jlab\\Epas\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jlab\\Epas\\Tests\\": "tests/"
        }
    },
    "repositories": {
        "laravel-utilities": {
        "type": "vcs",
        "url": "https://github.com/JeffersonLab/laravel-utilities.git"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jlab\\Epas\\EpasServiceProvider"
            ],
            "aliases": {
                "Epas": "Jlab\\Epas\\Facades\\Epas"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
